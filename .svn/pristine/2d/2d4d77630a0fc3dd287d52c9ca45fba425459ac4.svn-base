                    <div class="card-body">
                        <div class="custom-tab-content px-3">
                          <div>
                            <form>
                                <div class="form-group row">
                                    <label for="beneficiaryNm" class="col-sm-2 col-form-label">Beneficiary Name</label>
                                    <div class="col-sm-2">
                                        <p><strong>Raghar Sharma</strong></p>
                                    </div>
                                    <label for="blockingDt" class="col-sm-2 col-form-label">Blocking Date</label>
                                    <div class="col-sm-2">
                                        <div class="input-group mb-3">
                                            <input type="text" id="blockingDt" class="form-control datepicker" placeholder="dd mmm yyyy" aria-label="Username" aria-describedby="basic-addon1">
                                            <div class="input-group-prepend">
                                                <label for="blockingDt" class="input-group-text"><em class="icon-calendar"></em></label>
                                            </div>
                                        </div>
                                    </div>
                                    <label for="regDt" class="col-sm-2 col-form-label">Registration Date</label>
                                    <div class="col-sm-2">
                                        <p><strong>19 Aug 2018</strong></p>
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-sm-12">
                                        <div class="form-check form-check-inline">
                                            <input class="form-check-input" type="radio" name="packageType" id="Unspecified" value="Unspecified">
                                            <label class="form-check-label" for="Unspecified"><strong>Unspecified</strong></label>
                                        </div>
                                        <div class="form-check form-check-inline">
                                            <input class="form-check-input" type="radio" name="packageType" id="preApproved" value="preApproved" checked>
                                            <label class="form-check-label" for="preApproved"><strong>Pre-approved</strong></label>
                                        </div>
                                        <div class="form-check form-check-inline">
                                            <input class="form-check-input" type="radio" name="packageType" id="Medical" value="Medical">
                                            <label class="form-check-label" for="Medical"><strong>Medical</strong></label>
                                        </div>
                                    </div>
                                </div>
                                <hr>
                                <div class="form-group row">
                                    <label for="packageHeader" class="col-sm-2 col-form-label">Package Header</label>
                                    <div class="col-sm-2">
                                        <select name="" id="packageHeader" class="form-control">
                                            <option value="0">Select</option>
                                            <option value="dental">DENTAL</option>
                                        </select>
                                    </div>
                                    <label for="packageDetail" class="col-sm-2 col-form-label">Package Detail</label>
                                    <div class="col-sm-2">
                                        <select name="" id="packageDetail" class="form-control">
                                            <option value="0">Select</option>
                                            <option value="1">Impacted Molar Including LA</option>
                                        </select>
                                    </div>
                                    <label for="treatmentCost" class="col-sm-2 col-form-label">Treatment Cost</label>
                                    <div class="col-sm-2">
                                        <input type="text" class="form-control" id="treatmentCost" placeholder="Enter here" />
                                    </div>
                                </div>
                                <div class="text-right py-3">
                                    <button class="btn btn-outline-primary my-2 my-sm-0" type="submit">Block</button>
                                </div>
                                <table class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Procedure</th>
                                            <th>Package</th>
                                            <th>Package Code</th>
                                            <th>Days</th>
                                            <th>Amount Blocked</th>
                                            <th>Invoice No.</th>
                                            <th>Blocking Date</th>
                                            <th>Description</th>
                                            <th>Remove</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>DENTAL</td>
                                            <td>Impacted Molar Including LA</td>
                                            <td>13</td>
                                            <td>2</td>
                                            <td>500</td>
                                            <td>124</td>
                                            <td>10 Sep 2018</td>
                                            <td>Desc Text</td>
                                            <td>
                                                <button type="button" class="btn btn-outline-danger btn-sm"><em class="icon-close"></em></button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                                <div class="text-right py-3">
                                    <a href="block-package-list.html" class="btn btn-outline-danger my-2 my-sm-0">Back</a>
                                    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Add Package</button>
                                </div>
                            </form>
                          </div>
