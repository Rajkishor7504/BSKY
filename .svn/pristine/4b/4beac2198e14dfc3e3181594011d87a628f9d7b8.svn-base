.loader {
    position: fixed;
    z-index: 999;
    height: 100%;
    width: 100%;
    top: 0;
    background-color: rgba(0,0,0,0.5);
    display: none;
}

.center {
    z-index: 1000;
    margin: 50px auto;
    padding: 10px;
    width: 200px;
    position: relative;
    top: calc(50% - 100px);
    background: #fff;
}

@keyframes animate-in {
    /* You could think of as "step 1" */
    0% {
        opacity: 0;
    }
    /* You could think of as "step 2" */
    100% {
        opacity: 1;
    }
}

@keyframes animate-left {
    /* You could think of as "step 1" */
    0% {
        left: -100px;
        opacity: 0;
    }
    /* You could think of as "step 2" */
    100% {
        opacity: 1;
        left: 0px;
    }
}

@keyframes animate-bottom {
    /* You could think of as "step 1" */
    0% {
        bottom: -100px;
        opacity: 0;
    }
    /* You could think of as "step 2" */
    100% {
        opacity: 1;
        bottom: 0px;
    }
}
