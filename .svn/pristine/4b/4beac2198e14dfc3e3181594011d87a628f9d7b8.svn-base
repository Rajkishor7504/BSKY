body {
    background-color: #F5F5F5;
    font-size: 16px;
}

h1, h2, h3, h4, h5, h6, p {
    font-family: 'Montserrat', sans-serif;
}

img {
    max-width: 100%;
}

.custom-tab {
    position: relative;
    border-bottom: 0px;
    z-index: 9;
}

a {
    color: #00bcd4;
}

    a:hover {
        color: #017d8d;
    }

label input[type="checkbox"] {
    position: relative;
    top: 2px;
    margin-right: 5px;
}

.form-control {
    border: 0px;
    border-bottom: 1px solid #ced4da;
    border-radius: 0px;
    position: relative;
}

    .form-control:after {
        content: "";
        position: absolute;
        top: 100%;
        left: 0px;
        width: 100%;
        height: 1px;
        background: #27455b;
    }

    .form-control:focus {
        box-shadow: none;
    }

.input-group-append .input-group-text {
    background: transparent;
    border-top: 0px;
    border-right: 0px;
    border-radius: 0px;
}

.input-group-prepend .input-group-text {
    background: transparent;
    border-top: 0px;
    border-left: 0px;
    border-radius: 0px;
}

.loginBg {
    position: relative;
    background-color: #f2f2f2;
    background-image: url(../imgs/pattern.jpg);
    overflow: hidden;
}

    .loginBg > .row {
        height: 100vh;
        position: relative;
    }

        .loginBg > .row .col {
            height: 100%;
        }

        .loginBg > .row .logoInfo {
            z-index: 9;
        }

            .loginBg > .row .logoInfo ul {
                margin: 0px;
                padding: 0px;
                list-style: none;
            }

                .loginBg > .row .logoInfo ul li {
                    position: relative;
                    display: flex;
                    align-items: center;
                }

                    .loginBg > .row .logoInfo ul li img {
                        width: 76px;
                        margin-right: 16px;
                    }

                    .loginBg > .row .logoInfo ul li h2 {
                        color: #000;
                        font-size: 0.8em;
                        font-weight: bold;
                        margin: 0px 0px 5px;
                    }

                    .loginBg > .row .logoInfo ul li p {
                        color: rgba(0, 0, 0, 0.7);
                        font-size: 0.8em;
                        margin: 0px;
                    }

            .loginBg > .row .logoInfo .logImg {
                width: 300px;
                max-width: 80%;
                position: relative;
                animation: animate-in 1.8s;
            }

        .loginBg > .row .card {
            border: 0px;
            box-shadow: none;
            position: relative;
            min-height: 550px;
            background-color: transparent;
            background-image: url(../imgs/login-bg.png);
            background-repeat: no-repeat;
            background-position: bottom center;
            background-size: 600px;
        }

            .loginBg > .row .card .card-body {
                width: 336px;
                margin: 0px auto;
            }

                .loginBg > .row .card .card-body > h5 {
                    color: #27455b;
                    font-weight: bold;
                    text-transform: uppercase;
                    margin-top: -20px;
                }

            .loginBg > .row .card input.form-control {
                border-width: 0px;
                border-bottom-width: 1px;
                border-radius: 0px;
            }

            .loginBg > .row .card .btn {
                color: #02b4be;
            }

                .loginBg > .row .card .btn:hover {
                    color: #27455b;
                }

                .loginBg > .row .card .btn.loginBtn {
                    border: 1px solid #02b4be;
                    display: block;
                    background: #fff;
                }

                    .loginBg > .row .card .btn.loginBtn:hover {
                        text-decoration: none;
                        color: #000;
                        background-color: #02b4be;
                    }

.navbar {
    background-color: #27455b;
}

    .navbar .row {
        width: 100%;
    }

        .navbar .row .navbar-toggler {
            border: 0px;
            padding-left: 0px;
        }

            .navbar .row .navbar-toggler:hover {
                color: #fff;
            }

            .navbar .row .navbar-toggler:focus {
                outline: 0px;
            }

        .navbar .row .header-logo {
            margin: 0px;
            padding: 0px;
            list-style: none;
        }

            .navbar .row .header-logo li {
                position: relative;
                display: flex;
                align-items: center;
            }

                .navbar .row .header-logo li img {
                    width: 56px;
                    margin-right: 16px;
                }

                .navbar .row .header-logo li h2 {
                    color: #fff;
                    font-size: 0.8em;
                    margin: 0px 0px 5px;
                    font-weight: bold;
                }

                .navbar .row .header-logo li p {
                    color: rgba(255, 255, 255, 0.7);
                    font-size: 0.8em;
                    margin: 0px;
                }

        .navbar .row .header-menu > li > a {
            color: #fff;
        }

            .navbar .row .header-menu > li > a.active {
                background: #fff;
                color: #27455b;
                border-radius: 3px;
            }

        .navbar .row .site-title {
            color: #fff;
            margin: 0px;
            font-size: 1.2em;
            position: relative;
            top: -2px;
        }

        .navbar .row .action-area .header-btn-wrapper {
            display: inline-block;
            position: relative;
        }

            .navbar .row .action-area .header-btn-wrapper .card {
                position: absolute;
                top: 100%;
                right: 0px;
                min-width: 350px;
                max-height: 450px;
                overflow: hidden;
                border: 0px;
                border-radius: 0px;
                box-shadow: 0px 1px 4px #999;
                z-index: 999;
            }

                .navbar .row .action-area .header-btn-wrapper .card:hover {
                    overflow-y: auto;
                }

                .navbar .row .action-area .header-btn-wrapper .card::-webkit-scrollbar-track {
                    -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
                    background-color: #F5F5F5;
                }

                .navbar .row .action-area .header-btn-wrapper .card::-webkit-scrollbar {
                    width: 6px;
                    background-color: #F5F5F5;
                }

                .navbar .row .action-area .header-btn-wrapper .card::-webkit-scrollbar-thumb {
                    background-color: #000000;
                }

                .navbar .row .action-area .header-btn-wrapper .card .list-group .list-group-item {
                    border-left: 0px;
                    border-right: 0px;
                    font-size: 0.84em;
                }

                    .navbar .row .action-area .header-btn-wrapper .card .list-group .list-group-item h5 {
                        font-size: 1.2em;
                        color: #000;
                    }

                    .navbar .row .action-area .header-btn-wrapper .card .list-group .list-group-item small {
                        color: #999;
                    }

            .navbar .row .action-area .header-btn-wrapper > button {
                color: rgba(255, 255, 255, 0.9);
                position: relative;
                padding-top: 14px;
            }

                .navbar .row .action-area .header-btn-wrapper > button i {
                    font-size: 1.6em;
                }

                .navbar .row .action-area .header-btn-wrapper > button .badge {
                    position: absolute;
                    right: 0px;
                    top: 6px;
                    font-size: 0.7em;
                }

                .navbar .row .action-area .header-btn-wrapper > button:hover {
                    text-decoration: none;
                }

        .navbar .row .action-area .logedUser button:after {
            display: none;
        }

        .navbar .row .action-area .logedUser button img {
            width: 44px;
            height: 44px;
            border-radius: 50%;
        }

        .navbar .row .action-area .logedUser .dropdown-menu {
            left: auto;
            right: 0px;
        }

            .navbar .row .action-area .logedUser .dropdown-menu .dropdown-item {
                padding: .25rem 1rem;
            }

.secondary-menu-bar {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 16px;
    background-color: #e9ecef;
}

    .secondary-menu-bar .secondary-menu {
        margin: 0px;
        padding: 0px;
        list-style: none;
        display: flex;
        justify-content: flex-end;
    }

        .secondary-menu-bar .secondary-menu li a {
            display: block;
            margin-left: 10px;
        }

            .secondary-menu-bar .secondary-menu li a img {
                transition: all .2s ease-in-out;
                width: 30px;
                height: 30px;
            }

            .secondary-menu-bar .secondary-menu li a:hover img, .secondary-menu-bar .secondary-menu li a.active img {
                transform: scale(1.1);
            }

.breadcrumb-item + .breadcrumb-item::before {
    color: #9E9E9E !important;
}

.hospital-name {
    color: #27455b;
    font-size: 1.6em;
    text-transform: uppercase;
}

    .hospital-name img {
        display: block;
        width: 56px;
        margin: 0px auto 16px;
    }

.dashboard-menu .col a {
    display: block;
    text-align: center;
}

    .dashboard-menu .col a img {
        width: 100px;
        transition: all .2s ease-in-out;
    }

    .dashboard-menu .col a p {
        transition: all .2s ease-in-out;
        text-transform: uppercase;
        margin-top: 16px;
        color: #666;
        font-size: 1.2em;
    }

    .dashboard-menu .col a:hover {
        text-decoration: none;
    }

        .dashboard-menu .col a:hover img {
            transform: scale(1.2);
        }

        .dashboard-menu .col a:hover p {
            color: #000;
        }

