    <script type="text/javascript">
        ServiceURL = "@ConfigurationManager.AppSettings["ServiceURL"]";
        ShortcutKeyURL = "@ConfigurationManager.AppSettings["ShortcutKeyURL"]";
        AppURL="@ConfigurationManager.AppSettings["PortalURL"]";
    </script>
    <script src="~/js/ShortcutKey.js"></script>
    <script type="text/javascript">
        function gettoken() { 
                 var token = '@Html.AntiForgeryToken()';
                 token = $(token).val();
                 return token;
        }
        var sessionHospitalCode = '@Session["HospitalCode"]';
        $(document).ready(function () {
            if ('@Session["UserName"]'== 'MIS_02005038') {
                $('#logTools').show();
                $('#Tools').hide();
            }
            else {
                $('#logTools').hide();
                $('#Tools').show();
            }
        });
    </script>
    <script>
        $(function () {
            $('body').on('focus', '.focusedBtn', function () {
                $(this).parents('tr').addClass('activeRow');
            });
            $('body').on('blur', '.focusedBtn', function () {
                $(this).parents('table').find('tr').removeClass('activeRow');
            });
        });
    </script>
</head>
