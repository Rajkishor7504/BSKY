        function getPatientStatistics() {
            //debugger;
            var PatientStatsParams = {
                ACTION: 'B',
                HospitalCode:@Session["HospitalCode"]
            };
            $.ajax({
                url: ServiceURL + "/api/Common/getPatientStats",
                type: "GET",
                data: PatientStatsParams,
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                beforeSend: function () {
                    $("#loader").show();
                },
                success: function (result) {
                    //console.log(result);
                    $('#BlockPackagePS').html(result.block);
                    $('#UnblockPackagePS').html(result.unblock);
                    $('#PreAuthPendingPS').html(result.pedingpre_auth);
                    $('#DischargePS').html(result.discharge);
                },
                complete: function () {
                    $("#loader").hide();
                },
                error: function (error) {
                    console.log(error.responseText);
                }
            });
        }

        function tictic() {
            $('.tictic').vTicker({
                showItems: 3,
                speed: 1000,
                pause: 1000,
                animate: true,
                //autoplay:false;
                margin: 0,
                padding: 0
            });
        }
    });
</script>


