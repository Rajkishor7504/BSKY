
@{
    ViewBag.Title = "HospitalPackageReport";
    Layout = "~/Views/Shared/_CommonLayout.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <div class="clearfix"></div>
                    <!-- Family Verification info -->
                    <div class="form-box-bg mb-4">
                        <div class="row">
                            <div class="col-xl-12 col-md-12 col-sm-12">
                                <div class="tab-admin">
                                    <ul class="nav nav-tabs" role="tablist">
                                        <li class="nav-item">
                                            <a class="nav-link active" data-toggle="tab" href="#tabs-1" role="tab"> Package Block Report </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" onclick="getDefaultReport('tUnblock');" data-toggle="tab" href="#tabs-2" role="tab"> Package Un-Block report  </a>
                                            <input id="hdnTUnblock" type="hidden" value="0" />
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" onclick="getDefaultReport('tDischarge');" data-toggle="tab" href="#tabs-3" role="tab"> Package Discharge Report  </a>
                                            <input id="hdnTDischarge" type="hidden" value="0" />
                                        </li>
                                    </ul>
                                    <!-- tabing section start -->
                                    <div class="tab-content">
                                        <div class="tab-pane active" id="tabs-1" role="tabpanel">
                                            <!-- Admission Report -->
                                            <div class="col-md-12 mt-3 border-bottom">
                                                <div class="form-row">
                                                    <div class="col-lg-3">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1">Is Pre-Auth :</label>
                                                            <select id="ddIsPreAuth1" class="form-control form-group">
                                                                <option value="0">-Select-</option>
                                                                <option value="Y">Yes</option>
                                                                <option value="N">No</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1"> From Date :</label>
                                                            <input type="text" id="txtFromDate1" class="form-control datepicker">
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1"> To Date :</label>
                                                            <input type="text" id="txtToDate1" class="form-control datepicker">
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1">Date Type :</label>
                                                            <select id="ddDateType1" class="form-control form-group">
                                                                <option value="AD">Actual Admission Date</option>
                                                                <option value="BD">Package Block Date</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1">Blocking Mode :</label>
                                                            <select id="ddBlockingMode1" class="form-control form-group">
                                                                <option value="0">-Select-</option>
                                                                <option value="Y">Emergency</option>
                                                                <option value="N">PreAuth/Surgery</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1">Verification Mode :</label>
                                                            <select id="ddAuthType1" class="form-control form-group">
                                                                <option value="0">-Select-</option>
                                                                <option value="1">POS</option>
                                                                <option value="2">IRIS</option>
                                                                <option value="3">OTP</option>
                                                                <option value="4">OVERRIDE</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <div> <label class="pkg-label mb-1"> &nbsp; </label> </div>
                                                        <input id="btnSearch1" type="button" onclick="getPackageBlockReport();" class="btn btn-outline-success" value="Search">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xl-12 col-lg-12 mt-3 heading-pkg ">
                                                <div class="d-flex">
                                                    <div class="flex-fill">
                                                        <h5> Package Block Report </h5>
                                                    </div>
                                                    <div>
                                                        <button id="btnExcel1" type="button" class="btn btn-outline-success btn-sm float-right" data-toggle="tooltip" title="Export To Excel">
                                                            <i class="fa fa-file-excel-o"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="table-responsive">
                                                <table id="grdPackageBlock" class="table table-striped preauth-tbl" cellspacing="0" width="100%"></table>
                                            </div>
                                            <!-- Admission Report -->
                                        </div>

                                        <div class="tab-pane" id="tabs-2" role="tabpanel">
                                            <div class="col-md-12 mt-3 border-bottom">
                                                <div class="form-row">
                                                    <div class="col-lg-2">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1">Is Pre-Auth :</label>
                                                            <select id="ddIsPreAuth2" class="form-control form-group">
                                                                <option value="0">-Select-</option>
                                                                <option value="Y">Yes</option>
                                                                <option value="N">No</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-2">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1"> From Date :</label>
                                                            <input type="text" id="txtFromDate2" class="form-control datepicker">
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-2">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1"> To Date :</label>
                                                            <input type="text" id="txtToDate2" class="form-control datepicker">
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-2">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1">Date Type :</label>
                                                            <select id="ddDateType2" class="form-control form-group">
                                                                <option value="BD">Package Block Date</option>
                                                                <option value="UD">Package Unblock Date</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-2">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1">Verification Mode :</label>
                                                            <select id="ddAuthType2" class="form-control form-group">
                                                                <option value="0">-Select-</option>
                                                                <option value="1">POS</option>
                                                                <option value="2">IRIS</option>
                                                                <option value="3">OTP</option>
                                                                <option value="4">OVERRIDE</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-2">
                                                        <div> <label class="pkg-label mb-1"> &nbsp; </label> </div>
                                                        <input id="btnSearch2" type="button" onclick="getPackageUnblockReport();" class="btn btn-outline-success" value="Search">
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-xl-12 col-lg-12 mt-3 heading-pkg ">
                                                <div class="d-flex">
                                                    <div class="flex-fill">
                                                        <h5> Package Un-block Report </h5>
                                                    </div>
                                                    <div>
                                                        <button id="btnExcel2" type="button" class="btn btn-outline-success btn-sm float-right" data-toggle="tooltip" title="Export To Excel">
                                                            <i class="fa fa-file-excel-o"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>


                                            <!-- Un-Blocking Report -->
                                            <div class="table-responsive">
                                                <table id="grdPackageUnblock" class="table table-striped preauth-tbl" cellspacing="0" width="100%"></table>
                                            </div>
                                            <!-- Un-Blocking Report -->
                                        </div>

                                        <div class="tab-pane" id="tabs-3" role="tabpanel">
                                            <div class="col-md-12 mt-3 border-bottom">
                                                <div class="form-row">
                                                    <div class="col-lg-2">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1">Is Pre-Auth :</label>
                                                            <select id="ddIsPreAuth3" class="form-control form-group">
                                                                <option value="0">-Select-</option>
                                                                <option value="Y">Yes</option>
                                                                <option value="N">No</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-2">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1"> From Date :</label>
                                                            <input type="text" id="txtFromDate3" class="form-control datepicker">
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-2">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1"> To Date :</label>
                                                            <input type="text" id="txtToDate3" class="form-control datepicker">
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-2">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1">Date Type :</label>
                                                            <select id="ddDateType3" class="form-control form-group">
                                                                <option value="PD">Package Discharge Date</option>
                                                                <option value="DD">Actual Discharge Date</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-2">
                                                        <div class="mb-2">
                                                            <label class="pkg-label mb-1">Verification Mode :</label>
                                                            <select id="ddAuthType3" class="form-control form-group">
                                                                <option value="0">-Select-</option>
                                                                <option value="1">POS</option>
                                                                <option value="2">IRIS</option>
                                                                <option value="3">OTP</option>
                                                                <option value="4">OVERRIDE</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-2">
                                                        <div> <label class="pkg-label mb-1"> &nbsp; </label> </div>
                                                        <input id="btnSearch3" type="button" onclick="getPackageDischargeReport();" class="btn btn-outline-success" value="Search">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xl-12 col-lg-12 mt-3 heading-pkg ">
                                                <div class="d-flex">
                                                    <div class="flex-fill">
                                                        <h5> Package Discharge Report </h5>
                                                    </div>
                                                    <div>
                                                        <button id="btnExcel3" type="button" class="btn btn-outline-success btn-sm float-right" data-toggle="tooltip" title="Export To Excel">
                                                            <i class="fa fa-file-excel-o"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- Discharge Report -->
                                            <div class="table-responsive">
                                                <table id="grdPackageDischarge" class="table table-striped preauth-tbl" cellspacing="0" width="100%"></table>
                                            </div>
                                        </div>
                                        <!-- Discharge Report -->
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- tabing section end -->
<script src="~/js/dtButtons.min.js"></script>
<script src="~/js/jszip.min.js"></script>
<script src="~/js/buttonsHtml5.min.js"></script>

<script>
    document.mynamespace = {};
    document.mynamespace.hospitalcode = '@HttpContext.Current.Session["HospitalCode"]';
    var hospitalcode = document.mynamespace.hospitalcode;
</script>

@Scripts.Render("~/bundles/HospitalPackageReport")
